library(shinyalert)


ui <- fluidPage(theme = shinytheme("simplex"),
                tags$head(
                  tags$link(rel = "stylesheet", type = "text/css", href = "main.css")
                ),
                uiOutput('mainBody')
  
)

server <- function(input, output, session) {
  
  output$mainBody<-renderUI({
    
  })
  
}

shinyApp(ui, server)